{
  "compilerOptions": {
    "target": "ESNext",                          
    "module": "NodeNext",
    "moduleResolution": "NodeNext",                    
    //"lib": [],
    "allowSyntheticDefaultImports": true,                     
    "allowJs": true,
    "outDir": "build",                          
    "rootDir": "src",
    "strict": true,         
    "noImplicitAny": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false
  },

}